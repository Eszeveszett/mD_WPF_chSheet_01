<Window x:Class="mD_WPF_chSheet_01.windows.drawnCharacterSelectorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mD_WPF_chSheet_01.windows"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="Choose your party members" Height="900" Width="1600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>

        <Frame x:Name="FRM_Dcsw" NavigationUIVisibility="Hidden" 
            Grid.Row="0"/>
        <StackPanel Orientation="Horizontal" Grid.Row="1">
            <Button x:Name="BTN_Back" Click="BTN_Back_Click"
                Content="Back" Margin="5" Padding="5"/>
            <!--<Button x:Name="BTN_Select" Click="BTN_Select_Click"
                Content="Select Start" Margin="5" Padding="5"/>-->
        </StackPanel>
    </Grid>
</Window>
